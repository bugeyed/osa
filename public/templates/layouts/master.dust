<!doctype html>

<html lang="en" ng-app="OSA">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
  <title>Open Source Aquatics Aquarium Controller</title>
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.21/angular.min.js"></script>
  <script type="text/javascript" src="js/angular-route.js"></script>
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
  <link rel="stylesheet" href="css/styles.css">

</head>
<body>
	<div id="off-canvas" off-canvas></div>
	<div class="container h100">
		<header ng-controller="headerController">
			<button type="button" class="navbar-toggle" ng-click="toggleMenu()">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
    </header>

		<div ng-view class="h100"></div>

	</div>
	<script src="/socket.io/socket.io.js"></script>
	<script src="js/bootstrap/src/timepicker/timepicker.js"></script>
	<script src="js/bootstrap/src/buttons/buttons.js"></script>
	<script src="js/bootstrap/src/transition/transition.js"></script>
	<script src="js/bootstrap/src/modal/modal.js"></script>
	<script src="js/outlet/outlet.js"></script>
	<script src="js/pump/pump.js"></script>
	<script src="js/offcanvas/offcanvas.js"></script>
	<script src="js/app.js"></script>
</body>
</html>


